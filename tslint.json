{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "no-floating-promises": [
      true,
      "DocumentQuery",
      "Query",
      "MongooseThenable"
    ],
    "no-namespace": false,
    "no-submodule-imports": [
      false,
      "@material-ui/core",
      "next"
    ]
  }
}